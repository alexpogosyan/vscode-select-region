{
  "name": "vscode-select-region",
  "displayName": "Select Region",
  "description": "Emacs-style set mark and select arbitrary region",
  "publisher": "alexpogosyan",
  "version": "0.0.2",
  "icon": "icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexpogosyan/vscode-select-region.git"
  },
  "keywords": [
    "emacs",
    "keybindings",
    "select region"
  ],
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Keymaps"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "select-region.toggleMark",
        "title": "Set start mark"
      }
    ],
    "keybindings": [
      {
        "command": "select-region.toggleMark",
        "key": "cmd-alt-k",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  }
}
